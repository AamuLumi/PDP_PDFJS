{
  "name": "PDFJS",
  "version": "0.0.0",
  "description": "A PDF Creator in JavaScript",
  "private": true,
  "scripts": {
    "start": "meteor",
    "tests-unit": "./node_modules/chimp/bin/chimp.js --mocha --ddp=http://localhost:3000 --path=tests/unit_tests",

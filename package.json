{
  "name": "PDFJS",
  "version": "0.0.0",
  "description": "A PDF Creator in JavaScript",
  "private": true,
  "scripts": {
    "start": "meteor",
    "tests-unit": "cd tests/unit_tests/ && npm install && npm start",
    "tests-unit-watch": "./node_modules/chimp/bin/chimp.js --mocha --watch --ddp=http://localhost:3000 --path=tests/unit_tests",

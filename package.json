    "tests": "./node_modules/chimp/bin/chimp.js --ddp=http://localhost:3000 --path=tests/acceptance_tests",
    "tests-watch": "./node_modules/chimp/bin/chimp.js --ddp=http://localhost:3000 --watch --path=tests/acceptance_tests",
    "tests-perf": "cd tests/load_tests/ && npm install && npm start",
    "tests-unit": "cd tests/unit_tests/ && npm install && npm start"

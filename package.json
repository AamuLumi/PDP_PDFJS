  "dependencies": {
    "adm-zip": "^0.4.7",
    "base64-stream": "^0.1.3",
    "chimp": "^0.31.1",
    "libxml-xsd": "^0.5.2",
    "nodexml": "^1.0.2",
    "pdfmake": "^0.1.18",
    "xml2js": "^0.4.16"
  }

{
  "name": "PDFJS",
  "version": "0.0.0",
  "description": "A PDF Creator in JavaScript",
  "private": true,
  "scripts": {
    "start": "meteor",
    "unit-tests": "meteor test --driver-package practicalmeteor:mocha --port 3100",
    "acceptance-tests": "./node_modules/chimp/bin/chimp.js --ddp=http://localhost:3000 --path=tests/acceptance_tests",
    "tests-watch": "./node_modules/chimp/bin/chimp.js --ddp=http://localhost:3000 --watch --path=tests/acceptance_tests",
    "load-tests": "cd tests/load_tests/ && npm install && npm start"
  },
  "author": "Florian Kauder <flokauder@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "base64-stream": "^0.1.3",
    "browser": "^0.2.6",
    "chimp": "^0.31.1",
    "libxml-xsd": "^0.5.2",
    "nodexml": "^1.0.2",
    "pdfmake": "^0.1.18",
    "xml2js": "^0.4.16"
  }
}
